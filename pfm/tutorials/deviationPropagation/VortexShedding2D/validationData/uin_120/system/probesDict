FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      probesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



fields
(
    U
    UMean
    UPrime2Mean
);

writeControl   timeStep;
writeInterval  1;


// Locations to be probed.
probeLocations
(
    (2.0 0.0 0.0)
    (5.0 0.0 0.0)
    (10.0 0.0 0.0)
);

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
