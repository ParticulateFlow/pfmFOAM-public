FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
    //default         backward;
    //default         CrankNicolson 0.5;
}

gradSchemes
{
    default         Gauss linear;
    //grad(p)         Gauss linear;

    //default 	     leastSquares;

    //default 	     cellMDLimited leastSquares 0.5;
    //default 	     cellMDLimited Gauss linear 1;

    grad(U) 	     cellMDLimited Gauss linear 1;
}

divSchemes
{
    default         none;
    convectiveTermError Gauss linear;
    div(U)      Gauss linear;
    div(phi,U)      Gauss linearUpwindV grad(U) ;
    div(phi_ts,X_uu) Gauss linearUpwindV grad(X_uu) ;
    div(X_uu) Gauss linear;
    div((interpolate(deltaU)&S),deltaU) Gauss linear;
    //div(phi,U)      Gauss limitedLinearV 1;
    //div(phi,U)      Gauss linearUpwindV default;
    //div(phi,U)      Gauss linearUpwindV grad(U);
    div((nuEff*dev2(T(grad(X_uu))))) Gauss linear;
}

laplacianSchemes
{
//     default         Gauss linear limited 1;
     default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         limited 1;
}

