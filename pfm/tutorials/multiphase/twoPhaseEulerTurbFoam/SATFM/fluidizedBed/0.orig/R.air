FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    object      R.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0 0 0 0);

boundaryField
{
    vin_DB
    {
        type            zeroGradient;
    }

    pout
    {
        type            zeroGradient;
    }

    w_FB
    {
        type            fixedValue;
        value           $internalField;
    }

}

