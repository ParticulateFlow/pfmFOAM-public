FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    object      R.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0 0 0 0);

boundaryField
{
// gas inlet
    vin_g
    {
        type            zeroGradient;
    }
//outlet
    p_out 
    {
        type            zeroGradient;
    }
//solid inlet
    vin_s
    {
        type            zeroGradient;
    }
// walls
    wall
    {
        type            calculated;
        value           $internalField;
    }
}
