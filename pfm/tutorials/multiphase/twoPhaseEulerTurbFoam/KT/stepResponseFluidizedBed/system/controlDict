FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     twoPhaseEulerTurbFoam;

startFrom      latestTime;

startTime       0;

stopAt          endTime;

endTime         5.1;

deltaT          1e-5;

writeControl    adjustableRunTime;

writeInterval   0.02;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable on;

adjustTimeStep  yes;

maxCo           0.25;

maxDeltaT       2e-04;
minDeltaT	1e-5;

functions
{
    fieldAverage1
    {
        type            fieldAverage;
        functionObjectLibs ( "libfieldFunctionObjects.so" );
        writeControl   writeTime;
        fields
        (
            U.particles
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
            }

            U.air
            {
                 mean        on;
                 prime2Mean  off;
                 base        time;
            }

            T.particles
            {
                 mean        on;
                 prime2Mean  on;
                 base        time;
            }

            T.air
            {
                 mean        on;
                 prime2Mean  on;
                 base        time;
            }

            alpha.particles
            {
                 mean        on;
                 prime2Mean  on;
                 base        time;
            }
        );
    }
    /*probes
    {
        type            probes;
        functionObjectLibs ("libsampling.so");
        enabled         true;
        writeControl   timeStep;
        writeInterval  1;

        fields
        (

            U.particles
            alpha.particles

