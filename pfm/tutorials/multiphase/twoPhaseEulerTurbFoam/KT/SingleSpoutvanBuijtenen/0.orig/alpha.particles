FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    
    inlet_bg
    {
        type            zeroGradient;
    }


    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            zeroGradient;
    }
    wall2
    {
        type            zeroGradient;
    }

    
}

